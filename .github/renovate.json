{
  "extends": [
    "config:base"
  ],
  "labels": [ "dependencies" ],
  "dependencyDashboard": true,
  "packageRules": [
    {
      "matchManagers": ["gradle"],
      "registryUrls": [
        "https://cruglobal.jfrog.io/artifactory/maven-mobile/",
        "https://jitpack.io"
      ]
    },
    {
      "matchManagers": ["gradle"],
      "matchPackagePrefixes": ["org.jetbrains.kotlinx:kotlinx-coroutines-"],
      "groupName": "kotlinx.coroutines"
    }
  ]
}
